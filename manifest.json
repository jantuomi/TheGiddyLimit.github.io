{
	"manifest_version": 2,
	"default_locale": "en",
	"name": "5etools",
	"description": "A suite of browser-based tools for 5th Edition  Dungeons & Dragons players and Dungeon Masters.",
	"icons": [
		{
			"src": "icon.png",
			"type": "image/png",
			"sizes": "40x40"
		}
	],
	"start_url": "5etools.html",
	"background_color": "#ffffff",
	"display": "browser",
	"theme_color": "#006bc4",
	"permissions": [
		"clipboardWrite"
	]
}
